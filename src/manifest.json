{
    "name": "QuietDown 9GAG",
    "description": "Automatically adds the 'controls' attribute to all videos on 9gag.",
    "version": "1.0.0",
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": [
                "*://9gag.com/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_idle",
            "all_frames": true
        }
    ]
}